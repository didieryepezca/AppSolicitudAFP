    function dniDuplicado() {
        var vDni = $('#dni').val();

        var url = "/Solicitud/Duplicado";
        var data = { "dni": vDni };
        var dataType = "json";

        $.ajax({
            type: "POST",
            url: url,
            data: data,
            success: function (response) {
                console.log(response);
                if (response >= 1) {
                    $('#lblExiste').show();
                    setTimeout(
                    function () { $("#lblExiste").fadeOut(500); }, 3000)
                }
                else {
                    $('#lblInexiste').show();
