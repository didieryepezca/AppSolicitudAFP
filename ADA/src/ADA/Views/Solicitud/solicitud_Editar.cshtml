                        <div class="table-responsive" style="height:250px;" id="idDocTabla">

                            <table class="table table-hover m-b-0" id="example">
                                @{int con = 0;}
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>
                                            DOCUMENTO
                                        </th>
                                        <th>
                                            ESTADO
                                        </th>
                                        <th>
                                            OBSERVACION
                                        </th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in ViewBag.docsAdjuntos)
                                    {
                                        con = @con + 1;
                                        <tr>
                                            <td>
                                                @con
                                            </td>

                                            <td>
                                                <a href="~/uploads/@item.LINK" target="_blank">@item.LINK</a>
                                            </td>

                                            <td>
                                                @item.ESTADO
                                            </td>
                                            <th>
                                                @item.OBSERVACION
                                            </th>

                                        </tr>
                                    }
                                </tbody>
                            </table>

                        </div>

                    </div>
                </div>

            </div>

        </div>
    </div>

    <div class="col-lg-6 col-md-12 col-sm-12" id="idDocumento" style="display:none;">
