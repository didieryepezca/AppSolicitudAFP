    function dniDuplicado() {
        var vDni = $('#dni').val();

        var url = "/Solicitud/Duplicado";
        var data = { "dni": vDni };
        var dataType = "json";

        $.ajax({
            type: "POST",
            url: url,
            data: data,
            success: function (response) {
                console.log(response);
                if (response >= 1) {
                    $('#lblExiste').show();
                    setTimeout(
                    function () { $("#lblExiste").fadeOut(500); }, 3000)
                }
                else {
                    $('#lblInexiste').show();
                    setTimeout(
                    function () { $("#lblInexiste").fadeOut(500); }, 3000)
                }
            },
            dataType: dataType
        });
    };

    function longDoc() {

        console.log("entro");
        if ($("#tipoDocumento").val() == "01") {

            console.log("DNI");
            $("#dni").attr('maxlength', '8');
        }
        if ($("#tipoDocumento").val() == "04") {

            console.log("CARNET");
            $("#dni").attr('maxlength', '9');
        }

    };

    function estadoChange() {

        console.log("cambio estado");

        if ($("#solicitudCombo").val() == "101") {
            $('#idDocumento').show();
            $('#rechazoCombo').show();

            console.log("IMPROCEDENTE");
        }

        if ($("#solicitudCombo").val() == "100") {
            $('#idDocumento').hide();
            $('#rechazoCombo').hide();

            console.log("PENDIENTE");
        }

        if ($("#solicitudCombo").val() == "102") {

            $('#idDocumento').show();
            $('#idDictamen').hide();
            $('#rechazoCombo').hide();

            console.log("DUDA");
        }

        if ($("#solicitudCombo").val() == "103") {

            $('#idDocumento').show();
            $('#idDictamen').show();

            console.log("PRE-CONFORMIDAD");
        }

        if ($("#solicitudCombo").val() == "104") {

            $('#idDocumento').hide();


            console.log("OPPLUS");
        }

        if ($("#solicitudCombo").val() == "105") {
            $('#observ').show();
            $('#idDictamen').show();
            console.log("DEVUELTO");
        }

        if ($("#solicitudCombo").val() == "106") {
            $('#observ').hide();
            $('#idDictamen').hide();
            console.log("DESESTIMADO");
        }

        if ($("#solicitudCombo").val() == "107") {

            $('#idDocumento').show();
            $('#idDictamen').show();
            console.log("ASIGNADO A ABOGADO");
        }
        //else
        //{
        //    $('#observ').hide();
        //    $('#idDictamen').hide();
        // }

    };

    function adjuntar() {


        var fileUpload = $("#txtSubirA").get(0);
        var files = fileUpload.files;
        var data = new FormData();
        for (var i = 0; i < files.length ; i++) {
            data.append(files[i].name, files[i]);
        }

        var vCodigo = $('#idCodigo').val();
        var vAFP = $('#idAFP').val();
        //var vDescripcion = $('#txtSubirA').val();
        //var vLink = vCodigo + '-C-' + vDescripcion;
        var vTipoAdjunto = $('#tipoAdjunto').val();

        console.log(vCodigo);
        console.log(vAFP);
        //console.log(vDescripcion);
        //console.log(vLink);
        console.log(vTipoAdjunto);


        console.log("-----------------");

        //tabla de Documentos adjuntos
        var form = $('#idDocTabla');

        $.ajax({
            type: "POST",
            url: "/Solicitud/adjuntarArchivo?codigo=" + vCodigo + "&afp=" + vAFP + "&observacion=" + vTipoAdjunto,
            contentType: false,
            processData: false,
            data: data,
            success: function (jres) {

                if (jres == "OK") {

                    $('#fileOK').show();
                    setTimeout(
                         function () { $("#fileOK").fadeOut(500); }, 3000)

                    //Recargar tabla de documentos
                    $(form).fadeOut(1500, function () {

                        form.load(window.location.href + " #idDocTabla");
                        form.fadeIn().delay(2000);
                    });


                } else {
                    $('#fileERROR').show();
                    setTimeout(
                         function () { $("#fileERROR").fadeOut(500); }, 3000);
                }
            },
        });
        return false;
    };

    function diasLab() {

        var fechaDocSelected = $('#fechaDocumentacion').val();
        var vFechStart = new Date(fechaDocSelected);

        var vFechEnd = new Date();
        var dd = String(vFechEnd.getDate()).padStart(2, '0');
        var mm = String(vFechEnd.getMonth() + 1).padStart(2, '0'); //January is 0!
        var yyyy = vFechEnd.getFullYear();

        vFechEnd = dd + '/' + mm + '/' + yyyy;

        var url = "/Solicitud/ExcluirSabDom";
        var data = { startDate: fechaDocSelected, endDate: vFechEnd };
        var dataType = "json";

        $.ajax({
            type: "POST",
            url: url,
            data: data,
            success: function (response) {
                console.log(response);
                if (response <= 4) {

                    console.log("menor o igual a 4");
                    $('#lblFechaOk').show();
                    setTimeout(
                    function () { $("#lblFechaOk").fadeOut(500); }, 3000)
                }
                else {
                    console.log("mayor o igual a 3");
                    $('#lblFechaError').show();
                    var fechaDocSelected = $('#fechaDocumentacion').val("");
                    setTimeout(
                    function () { $("#lblFechaERROR").fadeOut(500); }, 3000)
                }
            },
            dataType: dataType
        });


    }


    function confirma() {
        console.log("hiciste click");
        $('#fileOK').show();
        setTimeout(
             function () { $("#fileOK").fadeOut(500); }, 3000)
    }


    function fn_Validar(cod) {

        var url = "/Solicitud/UpdateValidad";
        var data = { vCod: cod };
        var dataType = "json";


        var form = $('#validados');

        $.ajax({
            type: "POST",
            url: url,
            data: data,
            success: function (response) {
                if (response == "OK") {

                    $('#validOK').show();
                    setTimeout(
                         function () { $("#validOK").fadeOut(500); }, 3000)
                   
                    
                    $(form).fadeOut(1500, function () {

                        form.load(" #validados");
                        form.fadeIn().delay(2000);
                    });
                }
                else {
                    $('#validFALSE').show();
                    setTimeout(
                         function () { $("#validFALSE").fadeOut(500); }, 3000);
                }           
            },
            
        });

        return false;
    }


    function fn_Conforme() {

        
        var cod = $('#idCodigo').val();
        console.log(cod);

       

        var url = "/Solicitud/updateSolicitudConforme";
        var data = { vCod: cod };
        var dataType = "json";

        $.ajax({
            type: "POST",
            url: url,
            data: data,
            success: function (response) {
                if (response == "OK") {

                    alert("Se dio Conformidad a la solicitud");
                    console.log(response);
                }
                else
                {
                    alert("No se actualizo, La Solicitud ya esta conforme");

                 }

            },
            dataType: dataType
        });
    }

    function fn_DevueltoCyber() {

        var obser = $('#idObser').val();        

        var cod = $('#idCodigo').val();
        console.log(cod);

        var url = "/Solicitud/updateSolicitudDevueltoOpplus";
        var data = { vObser:obser, vCod: cod };
        var dataType = "json";

        $.ajax({
            type: "POST",
            url: url,
            data: data,
            success: function (response) {

                if (response == "OK") {

                    alert("Se devolvio la solicitud a CYBERGESTION");
                    console.log(response);
                }
                else {
                    alert("Ya se encuentra devuelta");

                }            
               

            },
            dataType: dataType
        });
    }
</script>

